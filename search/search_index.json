00b \u200b\u83b7\u5f97\u200b\u4e00\u4e2a\u200b\u503c\u200b\u7684\u200b\u65f6\u5019\u200b\u9700\u8981\u200b\u5f3a\u5236\u200b\u7c7b\u578b\u8f6c\u6362\u200b</p>"},{"location":"Chapt11/Chapt10/#_3","title":"\u6cdb\u578b\u200b\u662f\u200b\u4ec0\u4e48\u200b\uff1a","text":"<ul> <li>\u200b\u5f15\u5165\u200b\u53c2\u6570\u200b\u7c7b\u578b\u200b\uff0c\u200b\u5728\u200b\u7f16\u8bd1\u200b\u65f6\u200b\u8fdb\u884c\u200b\u7c7b\u65b0\u200b\u5b89\u5168\u200b\u68c0\u6d4b\u200b\uff0c\u200b\u5141\u8bb8\u200b\u6211\u4eec\u200b\u5728\u200b\u7f16\u8bd1\u200b\u65f6\u200b\u68c0\u6d4b\u200b\u5230\u200b\u975e\u6cd5\u200b\u7684\u200b\u6570\u636e\u7ed3\u6784\u200b</li> </ul>"},{"location":"Chapt11/Chapt10/#_4","title":"\u6cdb\u578b\u200b\u7c7b","text":"<ul> <li>\u200b\u5177\u6709\u200b\u4e00\u4e2a\u200b\u6216\u8005\u200b\u591a\u4e2a\u200b\u7c7b\u578b\u200b\u53d8\u91cf\u200b\u7684\u200b\u7c7b\u200b</li> <li>\u200b\u7c7b\u578b\u200b\u53c2\u6570\u200b\uff08\u200b\u7c7b\u578b\u200b\u53d8\u91cf\u200b\uff09\uff1a\u200b\u4f5c\u200b\u5360\u4f4d\u200b\u7b26\u200b\uff0c\u200b\u6307\u793a\u200b\u5206\u914d\u200b\u7684\u200b\u7c7b\u578b\u200b</li> <li>\u200b\u58f0\u660e\u200b\u6cdb\u578b\u200b\u4e0d\u80fd\u200b\u4f7f\u7528\u200b\u57fa\u672c\u200b\u6570\u636e\u7c7b\u578b\u200b\uff0cint $\\rightarrow$ Integer</li> </ul> <pre><code>public class Gene&lt;T,E&gt;{\n    public T t;\n    public E e;\n}\n//TEST\nGene&lt;String, Integer&gt; g3 = new Gene&lt;&gt;();\ng3.t = \"100\";\ng3.e = 100;\n</code></pre>"},{"location":"Chapt11/Chapt10/#_5","title":"\u6cdb\u578b\u200b\u65b9\u6cd5","text":"<p>\u200b\u58f0\u660e\u200b\u6cdb\u578b\u200b\u7684\u200b\u65b9\u6cd5\u200b\u624d\u200b\u662f\u200b\u6cdb\u578b\u200b\u65b9\u6cd5\u200b\uff0c\u200b\u65e0\u8bba\u200b\u8fd9\u4e2a\u200b\u65b9\u6cd5\u200b\u662f\u5426\u662f\u200b\u5728\u200b\u666e\u901a\u200b\u7c7b\u200b\u8fd8\u662f\u200b\u6cdb\u578b\u200b\u7c7b\u200b\u4e2d\u200b\u5b9a\u4e49\u200b\u7684\u200b - \u200b\u793a\u4f8b\u200b\uff1a\u200b\u975e\u6cdb\u200b\u578b\u200b\u7c7b\u200b\u4e2d\u200b\u5b9a\u4e49\u200b\u6cdb\u578b\u200b\u65b9\u6cd5\u200b</p> <pre><code>class Gen{\n    public &lt;T,E&gt; void fun1(E e){}\n    public &lt;T&gt; T fun2(T t){\n        return t;\n    }\n}\nGen g = new Gen()\ng.&lt;String&gt;fun2(\"str\");\n\u200b\u6216\u8005\u200b\ng,fun2(\"str\");\n</code></pre> <p>\u200b\u4f7f\u7528\u200b\u6cdb\u578b\u200b\u65b9\u6cd5\u200b\uff0c\u200b\u5728\u200b\u65b9\u6cd5\u200b\u540d\u200b\u6dfb\u52a0\u200b&lt;&gt;\u200b\u586b\u5165\u200b\u5177\u4f53\u200b\u7c7b\u578b\u200b\u7684\u200b\u503c\u200b\uff1b\u200b\u591a\u6570\u200b\u60c5\u51b5\u200b\u4e0b\u200b\uff0c\u200b\u4e5f\u200b\u53ef\u4ee5\u200b\u7701\u7565\u200b - \u200b\u793a\u4f8b\u200b\uff1a\u200b\u6cdb\u578b\u200b\u7c7b\u200b\u4e2d\u200b\u5b9a\u4e49\u200b\u6cdb\u578b\u200b\u65b9\u6cd5\u200b   -  \u200b\u7c7b\u200b\u7684\u200b\u9759\u6001\u200b\u6cdb\u578b\u200b\u65b9\u6cd5\u200b\uff0c\u200b\u4e0d\u5f97\u200b\u4f7f\u7528\u200b\u6cdb\u578b\u200b\u7c7b\u200b\u4e2d\u200b\u58f0\u660e\u200b\u7684\u200b\u6cdb\u578b\u200b\uff0c\u200b\u53ef\u4ee5\u200b\u72ec\u7acb\u200b\u58f0\u660e\u200b\u3002 \u200b\u5373\u200b\u9759\u6001\u65b9\u6cd5\u200b\u4e0d\u80fd\u200b\u83b7\u5f97\u200b\u8be5\u6cdb\u200b\u578b\u200b\u7c7b\u200b\u4e2d\u200b\u7684\u200b\u58f0\u660e\u200b\u7684\u200b\u6cdb\u578b\u200b\uff08\u200b\u56e0\u4e3a\u200b\u8fd9\u4e2a\u200b\u6cdb\u578b\u200b\u662f\u200b\u5728\u200b\u5b9e\u4f8b\u200b\u5316\u200b\u7684\u200b\u65f6\u5019\u200b\u624d\u200b\u786e\u5b9a\u200b\u7684\u200b\uff09</p> <pre><code>public class MyClass&lt;T&gt; {\n    T field;\n\n    public static &lt;E&gt; void myMethod(E arg) {\n        // ...\n    }\n}\n</code></pre> <p>\u200b\u4f8b\u5b50\u200b\uff1a  \u200b\u4f8b\u5b50\u200b\uff1a </p>"},{"location":"Chapt11/Chapt10/#_6","title":"\u6cdb\u578b\u200b\u63a5\u53e3","text":"<ul> <li>\u200b\u5b9a\u4e49\u200b\u6cdb\u578b\u200b\u63a5\u53e3\u200b</li> </ul> <pre><code>interface Gen &lt;T&gt;{\n    T fun1();\n}\n</code></pre> <ul> <li>\u200b\u6cdb\u578b\u200b\u63a5\u53e3\u200b\u7684\u200b\u5b9e\u73b0\u200b</li> <li>\u200b\u5b9e\u73b0\u200b\u7c7b\u4e3a\u200b\u975e\u6cdb\u200b\u578b\u200b\u7c7b\u200b\uff0c\u200b\u9700\u200b\u5177\u4f53\u200b\u6307\u5b9a\u200b\u63a5\u53e3\u200b\u7684\u200b\u6cdb\u578b\u200b     \u200b\u5b9e\u73b0\u200b\u7c7b\u200b\u5df2\u7ecf\u200b\u786e\u5b9a\u200b\u7c7b\u578b\u200b\uff0c\u200b\u53ef\u4ee5\u200b\u76f4\u63a5\u200b\u4f7f\u7528\u200b\uff08\u200b\u89c1\u200b\u4e0b\u56fe\u200b\uff09</li> <li>\u200b\u5b9e\u73b0\u200b\u7c7b\u4e3a\u200b\u6cdb\u578b\u200b\u7c7b\u200b\uff0c\u200b\u5b9e\u73b0\u200b\u7c7b\u200b\u7684\u200b\u6cdb\u578b\u200b\u8981\u200b\u4e0e\u200b\u63a5\u53e3\u200b\u4e00\u81f4\u200b     \u200b\u5b9e\u73b0\u200b\u7c7b\u200b\u672a\u786e\u5b9a\u200b\u7c7b\u578b\u200b\uff0c\u200b\u4f7f\u7528\u200b\u65f6\u200b\u786e\u5b9a\u200b\uff08\u200b\u89c1\u200b\u4e0b\u56fe\u200b\uff09</li> </ul> <pre><code>class Gen_impl implements Gen&lt;String&gt;{\n    @Override\n    public String fun1(){\n        return null;\n    }\n}\n</code></pre> <pre><code>class Gem_impl&lt;T&gt; implements Gen&lt;T&gt;{\n    @Override\n    public T fun1(){\n        return null;\n    }\n}\n</code></pre>"},{"location":"Chapt11/Chapt10/#_7","title":"\u6cdb\u578b\u200b\u7684\u200b\u901a\u914d\u7b26","text":""},{"location":"Chapt11/Chapt10/#_8","title":"\u610f\u4e49\u200b\uff1a","text":"<ul> <li>\u200b\u4e25\u683c\u200b\u7684\u200b\u6cdb\u578b\u200b\u7c7b\u578b\u200b\u7cfb\u7edf\u200b\u53c2\u6570\u200b\u4e00\u65e6\u200b\u6307\u5b9a\u200b\u4fbf\u200b\u65e0\u6cd5\u200b\u6539\u53d8\u200b</li> <li>\u200b\u9700\u8981\u200b\u5141\u8bb8\u200b\u7c7b\u578b\u200b\u53c2\u6570\u200b\u53d1\u751f\u53d8\u5316\u200b</li> <li>\u200b\u540c\u65f6\u200b\u9700\u8981\u200b\u63a7\u5236\u200b\u53ef\u4ee5\u200b\u6307\u5b9a\u200b\u7684\u200b\u7c7b\u578b\u200b</li> </ul>"},{"location":"Chapt11/Chapt10/#_9","title":"\u4f8b\u5b50\u200b\uff1a","text":""},{"location":"Chapt11/Chapt10/#_10","title":"\u5e38\u89c1\u200b\u6cdb\u578b\u200b\u901a\u914d\u7b26\u200b\u7c7b\u578b\u200b\uff1a\u200b\u5141\u8bb8\u200b\u7c7b\u578b\u200b\u53c2\u6570\u200b\u53d1\u751f\u53d8\u5316","text":"<ul> <li>&lt;? extends ClassName&gt;\uff1a\u200b\u7c7b\u578b\u200b\u53c2\u6570\u200b\u662f\u200bClass\u200b\u7684\u200b\u5b50\u7c7b\u200b\u6216\u8005\u200b\u5c31\u662f\u200bClass\u200b\u672c\u8eab\u200b\u7c7b\u578b\u200b</li> <li>&lt;? super ClassName&gt;\uff1a\u200b\u7c7b\u578b\u200b\u53c2\u6570\u200b\u662f\u200bClass\u200b\u7684\u200b\u7236\u7c7b\u200b\u6216\u8005\u200b\u5c31\u662f\u200bClass\u200b\u672c\u8eab\u200b\u7c7b\u578b\u200b</li> <li>&lt;?&gt; \u200b\u65e0\u9650\u200b\u5b9a\u200b\u901a\u914d\u7b26\u200b</li> </ul>"},{"location":"Chapt11/Chapt10/#_11","title":"\u6cdb\u578b\u200b\u7684\u200b\u8bbe\u8ba1\u200b:\u200b\u6a21\u677f\u200b\u65b9\u6cd5\u200b\u6a21\u5f0f","text":""},{"location":"Chapt11/Chapt10/#_12","title":"\u5185\u5bb9\u200b:","text":"<ul> <li>\u200b\u5b9a\u4e49\u200b\u4e00\u4e2a\u200b\u64cd\u4f5c\u200b\u4e2d\u200b\u7684\u200b\u7b97\u6cd5\u200b\u9aa8\u67b6\u200b</li> <li>\u200b\u5c06\u200b\u4e00\u4e9b\u200b\u6b65\u9aa4\u200b\u5ef6\u8fdf\u200b\u5230\u200b\u5b50\u7c7b\u200b\u4e2d\u200b</li> <li>\u200b\u6a21\u677f\u200b\u65b9\u6cd5\u200b\u6a21\u5f0f\u200b\u4f7f\u5f97\u200b\u5b50\u7c7b\u200b\u53ef\u4ee5\u200b\u4e0d\u200b\u6539\u53d8\u200b\u4e00\u4e2a\u200b\u7b97\u6cd5\u200b\u7684\u200b\u7ed3\u6784\u200b\u5373\u53ef\u200b\u91cd\u65b0\u200b\u5b9a\u4e49\u200b\u8be5\u200b\u7b97\u6cd5\u200b\u7684\u200b\u67d0\u4e9b\u200b\u7279\u5b9a\u200b\u6b65\u9aa4\u200b</li> </ul>"},{"location":"Chapt11/Chapt10/#_13","title":"\u5177\u4f53\u64cd\u4f5c\u200b\uff1a","text":""},{"location":"Chapt11/Chapt10/#_14","title":"\u7279\u70b9\u200b\uff1a","text":"<ul> <li>\u200b\u53cd\u5411\u200b\u63a7\u5236\u7ed3\u6784\u200b\uff1a</li> <li>\u200b\u7236\u7c7b\u200b\u8c03\u7528\u200b\u4e00\u4e2a\u200b\u5b50\u7c7b\u200b\u7684\u200b\u64cd\u4f5c\u200b</li> <li>\u201c\u200b\u597d\u83b1\u575e\u200b\u6cd5\u5219\u200b\u201d</li> <li>\u200b\u6ce8\u610f\u200b\uff1a</li> <li>\u200b\u6a21\u677f\u200b\u65b9\u6cd5\u200b\u5fc5\u987b\u200b\u6307\u660e\u200b\u54ea\u4e9b\u200b\u64cd\u4f5c\u200b\u662f\u200b\u94a9\u5b50\u200b\u64cd\u4f5c\u200b\uff08\u200b\u53ef\u200b\u88ab\u200b\u91cd\u65b0\u200b\u5b9a\u4e49\u200b\uff09\u200b\u4ee5\u53ca\u200b\u54ea\u4e9b\u200b\u662f\u200b\u62bd\u8c61\u200b\u64cd\u4f5c\u200b\uff08\u200b\u5fc5\u987b\u200b\u88ab\u200b\u91cd\u65b0\u200b\u5b9a\u4e49\u200b\uff09</li> <li>\u200b\u5c3d\u91cf\u51cf\u5c11\u200b\u4e00\u4e2a\u200b\u5b50\u7c7b\u200b\u5b9e\u73b0\u200b\u8be5\u7c7b\u200b\u5177\u4f53\u200b\u7b97\u6cd5\u200b\u7684\u200b\u8fc7\u7a0b\u200b\u4e2d\u200b\uff0c\u200b\u5fc5\u987b\u200b\u91cd\u65b0\u200b\u5b9a\u4e49\u200b\u7684\u200b\u90a3\u4e9b\u200b\u62bd\u8c61\u200b\u64cd\u4f5c\u200b\u7684\u200b\u6570\u76ee\u200b</li> </ul>"},{"location":"Chapt11/Chapt10/#_15","title":"\u53cd\u5c04\u200b\uff1a\u200b\u52a8\u6001\u200b\u83b7\u53d6\u4fe1\u606f\u200b\u3001\u200b\u52a8\u6001\u200b\u8c03\u7528\u200b\u5bf9\u8c61\u200b\u65b9\u6cd5\u200b\u7684\u200b\u529f\u80fd","text":""},{"location":"Chapt11/Chapt10/#_16","title":"\u9700\u8981\u200b\u53cd\u5c04\u200b\u7684\u200b\u65f6\u5019\u200b:\u200b\u4ece\u200b\u201c\u200b\u7c7b\u200b\u7684\u200b\u540d\u5b57\u200b\u201d\u200b\u5f97\u200b\u9053\u200b\u5177\u4f53\u200b\u7684\u200b\u7c7b","text":""},{"location":"Chapt11/Chapt10/#class","title":"Class\u200b\u7c7b\u200b\uff1a\u200b\u7c7b\u200b\u7684\u200b\u7c7b\u578b","text":"<p>Java\u200b\u8fd0\u884c\u200b\u65f6\u200b\u59cb\u7ec8\u200b\u4e3a\u200b\u6240\u6709\u200b\u5bf9\u8c61\u200b\u7ef4\u62a4\u200b\u4e00\u4e2a\u200b\u8fd0\u884c\u200b\u65f6\u200b\u7684\u200b\u7c7b\u578b\u200b\u6807\u8bc6\u7b26\u200b\uff08\u200b\u5373\u200bClass\uff09 \u200b\u4f1a\u200b\u8ddf\u8e2a\u200b\u8fd9\u4e9b\u200b\u8bf6\u200b\u5bf9\u8c61\u200b\u6240\u5c5e\u200b\u7684\u200b\u7c7b\u200b\u7684\u200b\u5b8c\u6574\u200b\u7684\u200b\u4fe1\u606f\u200b\uff0c\u200b\u6bd4\u5982\u200b\u6784\u9020\u65b9\u6cd5\u200b\u3001\u200b\u5305\u540d\u200b\u3001\u200b\u7c7b\u540d\u200b\u3001\u200b\u5b9e\u73b0\u200b\u7684\u200b\u63a5\u53e3\u200b\u3001\u200b\u62e5\u6709\u200b\u7684\u200b\u65b9\u6cd5\u200b\u548c\u200b\u5b57\u200b\u6bb5\u200b\u7b49\u200b  </p>"},{"location":"Chapt11/Chapt10/#class_1","title":"\u83b7\u53d6\u200bClass\u200b\u7c7b\u200b\u5bf9\u8c61\u200b\u7684\u200b\u4e09\u79cd\u200b\u65b9\u5f0f","text":"<ul> <li>Object.getClass()</li> <li>Class.forName(\u200b\u8868\u793a\u200b\u7c7b\u540d\u200b\u7684\u200b\u5b57\u7b26\u4e32\u200b\uff0c\u200b\u6bd4\u5982\u200b\uff1a\u201cReflect.Student\u201d)</li> <li>T.class(T\u200b\u53ef\u4ee5\u200b\u662f\u200b\u7c7b\u200b\u4e5f\u200b\u53ef\u4ee5\u200b\u4e0d\u662f\u200b\u7c7b\u200b)</li> </ul>"},{"location":"Chapt11/Chapt10/#_17","title":"\u901a\u8fc7\u200b\u53cd\u5c04\u200b\u6784\u9020\u200b\u7c7b\u200b\u7684\u200b\u5b9e\u4f8b","text":"<ul> <li>\u200b\u4f7f\u7528\u200bClass.newInstance() </li> <li>\u200b\u4f7f\u7528\u200bConstructor\u200b\u7684\u200bnewInstance </li> </ul> <pre><code>Student Harry = new Student (\u201cname\u201d, 10);\nClass StudentClass = Harry.getClass();\nConstructor con = StudentClass.getConstructor(String.class, int.class);\nStudent Ron = (Student)con.newInstance(\"name\", 10);\n</code></pre>"},{"location":"Chapt11/Chapt10/#_18","title":"\u901a\u8fc7\u200b\u53cd\u5c04\u200b\u6765\u200b\u83b7\u53d6\u200b\u548c\u200b\u4fee\u6539\u200b\u6210\u5458\u200b\u53d8\u91cf","text":"<p>getFields()\u200b\u4ec5\u4ec5\u53ea\u662f\u200b\u83b7\u5f97\u200b\u8fd9\u4e2a\u200b\u7c7b\u200b\u7684\u200b\u5b57\u200b\u6bb5\u200b\u6709\u200b\u54ea\u4e9b\u200b\uff0c\u200b\u4e0d\u80fd\u200b\u83b7\u5f97\u200b\u5177\u4f53\u200b\u5b9e\u4f8b\u200b\u7684\u200b\u5b57\u200b\u6bb5\u200b\u7684\u200b\u503c\u200b</p> <p></p>"},{"location":"Chapt11/Chapt10/#_19","title":"\u901a\u8fc7\u200b\u53cd\u5c04\u200b\u6765\u200b\u83b7\u53d6\u200b\u6210\u5458\u200b\u65b9\u6cd5","text":"<p>setAccessible(true)\u200b\u65b9\u6cd5\u200b\u5c31\u662f\u200b\u7528\u6765\u200b\u6539\u53d8\u200b\u8fd9\u79cd\u200b\u72b6\u6001\u200b\u7684\u200b\u3002\u200b\u5f53\u200b\u6211\u4eec\u200b\u8c03\u7528\u200bField\u200b\u5bf9\u8c61\u200b\u7684\u200bsetAccessible(true)\u200b\u65b9\u6cd5\u200b\u540e\u200b\uff0c\u200b\u8fd9\u4e2a\u200bField\u200b\u5bf9\u8c61\u200b\u4ee3\u8868\u200b\u7684\u200b\u5b57\u200b\u6bb5\u200b\u5c31\u200b\u53ef\u4ee5\u200b\u88ab\u200b\u8bbf\u95ee\u200b\u4e86\u200b\uff0c\u200b\u65e0\u8bba\u200b\u8fd9\u4e2a\u200b\u5b57\u200b\u6bb5\u200b\u539f\u672c\u200b\u7684\u200b\u8bbf\u95ee\u200b\u6743\u9650\u200b\u662f\u200b\u4ec0\u4e48\u200b\u3002 </p>"},{"location":"Chapt11/Chapt10/#_20","title":"\u76f8\u5173\u200b\u4f18\u7f3a\u70b9\u200b\uff1a","text":"<ul> <li>\u200b\u4f18\u70b9\u200b\uff1a</li> <li>\u200b\u7075\u6d3b\u200b\uff0c\u200b\u80fd\u591f\u200b\u5728\u200b\u8fd0\u884c\u200b\u65f6\u200b\u52a8\u6001\u200b\u83b7\u53d6\u200b\u7c7b\u200b\u7684\u200b\u5b9e\u4f8b\u200b</li> <li>\u200b\u7f3a\u70b9\u200b\uff1a</li> <li>\u200b\u53cd\u5c04\u200b\u76f8\u5f53\u4e8e\u200b\u4e00\u7cfb\u5217\u200b\u89e3\u91ca\u200b\u64cd\u4f5c\u200b\uff0c\u200b\u6027\u80fd\u6bd4\u200b\u76f4\u63a5\u200b\u7684\u200bJava\u200b\u4ee3\u7801\u200b\u8981\u200b\u6ee1\u200b\u5f88\u591a\u200b</li> <li>\u200b\u5b89\u5168\u200b\u95ee\u9898\u200b\uff1a\u200b\u53cd\u5c04\u200b\u673a\u5236\u200b\u7834\u574f\u200b\u4e86\u200b\u5c01\u88c5\u200b\u6027\u200b\uff0c\u200b\u901a\u8fc7\u200b\u53cd\u5c04\u200b\u80fd\u591f\u200b\u83b7\u53d6\u200b\u5e76\u200b\u8c03\u7528\u200b\u7c7b\u200b\u7684\u200b\u79c1\u6709\u200b\u65b9\u6cd5\u200b\u548c\u200b\u5b57\u200b\u6bb5\u200b</li> </ul>"},{"location":"Chapt11/Chapt10/#_21","title":"\u8bbe\u8ba1\u200b\u5b89\u5168\u200b\u7684\u200b\u5168\u5c40\u200b\u5355\u4f8b","text":""},{"location":"Chapt11/Chapt10/#_22","title":"\u5229\u7528\u200b\u53cd\u5c04\u200b\u7834\u574f\u200b\u5355\u4f8b\u200b\u6a21\u5f0f\u200b\uff1a","text":""},{"location":"Chapt11/Chapt10/#_23","title":"\u62b5\u5fa1\u200b\u53cd\u5c04\u200b\u7834\u574f","text":""}]}